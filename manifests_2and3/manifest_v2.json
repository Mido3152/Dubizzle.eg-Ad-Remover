{
  "name": "Dubizzle.eg Ad Remover",
  "version": "1.0.0",
  "manifest_version": 2,

  "id": "53a602e6-ccad-2001-18de-10bd940e4a42",
  "browser_specific_settings": {
    "gecko": {
      "id": "{53a602e6-ccad-2001-18de-10bd940e4a42}"
    }
  },

  "description": "A simple Chrome extension that removes ads and \"featured\" products from search results.",
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.dubizzle.com.eg/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["*://www.dubizzle.com.eg/*"],
  "author": "Mido315",
  "homepage_url": "https://github.com/Mido3152"
}
